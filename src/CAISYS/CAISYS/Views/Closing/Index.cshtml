
@{
    ViewData["Title"] = "AccountChart";
}

<div class="container">
    <div class="row m-4">
        <div class="col-5 text-center">
            <a asp-action="AddAccount" asp-controller="AccountChart">
                <img src="~/images/add-document_150x130.png" />
                <h4>بدء الاقفال</h4>
            </a>
        </div>

        <div class="col-5 text-center">
            <a asp-action="AddAccount" asp-controller="AccountChart" class="justify-content-center">
                <img src="~/images/edit-document.png"/>
                <h4 >قائمة الدخل</h4>
            </a>
        </div>
    </div>

    <div class="row m-4">
        <div class="col-5 text-center">
            <a asp-action="AddAccount" asp-controller="AccountChart">
                <img src="~/images/delete-button.png" />
                <h4>الارباح المبقاة</h4>
            </a>
        </div>
        <div class="col-5 text-center">
            <a asp-action="AddAccount" asp-controller="AccountChart">
                <img src="~/images/account.png" />
                <h4>قائمة المركز المالي</h4>
            </a>
        </div>
    </div>

    <div class="row m-4">
        <div class="col-5 text-center">
            <a asp-action="AddAccount" asp-controller="AccountChart">
                <img src="~/images/printer.png" />
                <h4>التدفقات النقدية</h4>
            </a>
        </div>
    </div>
</div>

