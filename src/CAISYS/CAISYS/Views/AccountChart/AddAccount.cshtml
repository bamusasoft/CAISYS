@model AddAccountVm
@{
    ViewData["Title"] = "AddAccount";
}

    <div class="container">
        <div class="row">
            <form asp-action="AddAcount" asp-controller="AccountChart" >
                <div asp-validation-summary="All" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="AccountNo"></label>
                    <input asp-for="AccountNo" autofocus />
                    <span asp-validation-for="AccountNo" class="text-danger"></span>
                </div>
            </form>
        </div>

    </div>
